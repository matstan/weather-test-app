<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">

    <listener>
        <listener-class>com.weather.test.app.gae.Hk2EnablementContextListener</listener-class>
    </listener>

    <servlet>
        <servlet-name>testServlet</servlet-name>
        <servlet-class>com.weather.test.app.gae.TestServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.weather.test.app.gae.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>parsingServlet</servlet-name>
        <servlet-class>com.weather.test.app.gae.ParsingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>testServlet</servlet-name>
        <url-pattern>/testServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/loginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>parsingServlet</servlet-name>
        <url-pattern>/parsingServlet</url-pattern>
    </servlet-mapping>
</web-app>
